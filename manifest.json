{
    "manifest_version": 3,
    "name": "KirotoRen",
    "version": "1.0",
    "description": "Select a portion of the screen.",
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon.png",
            "48": "icons/icon.png",
            "128": "icons/icon.png"
        }
    },
    "host_permissions": [
        "<all_urls>"
    ]
}